{
    "name": "fortress/behat-contexts",
    "description": "Behat contexts",
    "keywords": [
        "BDD",
        "Behat",
        "Symfony",
        "Context",
        "API Platform"
    ],
    "type": "library",
    "license": "beerware",
    "require": {
        "php": ">=8.0",
        "ext-dom": "*",
        "ext-libxml": "*",
        "ext-simplexml": "*",
        "behat/behat": "^3.8",
        "friends-of-behat/mink-extension": "^2.5",
        "justinrainbow/json-schema": "^5.2",
        "symfony/dom-crawler": "^4.4|^5.4|^6.0",
        "symfony/http-foundation": "^5.4|^6.0",
        "symfony/property-access": "^5.4|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Behatch\\": "src/"
        }
    },
    "config": {
        "bin-dir": "bin/"
    },
    "replace": {
        "sanpi/behatch-contexts": "self.version",
        "behatch/contexts": "self.version"
    }
}
